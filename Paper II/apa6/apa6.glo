\glossaryentry{v1.0> =General:>Initial release}{1}
\glossaryentry{v1.01> =General:>Prepended config files with `APA' to prevent possible conflicts along the \LaTeX  \ path.}{1}
\glossaryentry{v1.01> =General:>Minor changes in `apa6.ptex' to accommodate the \cs  {note} command.}{1}
\glossaryentry{v1.02> =General:>More reliable font-size selection}{1}
\glossaryentry{v1.02> =General:>Corrected default mode to jou}{1}
\glossaryentry{v1.11> =General:>New option `noextraspace'}{1}
