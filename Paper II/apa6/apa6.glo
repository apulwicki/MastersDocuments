\glossaryentry{v2.0> =General:>Fixed `headwidth' for `doc' mode}{1}
\glossaryentry{v2.01> =General:>Really fixed `headwidth' for `doc' mode}{1}
\glossaryentry{v2.05> =General:>Resolved the EndNote @@author-year specification causing no files to be found}{1}
\glossaryentry{v2.10> =General:>Package `biber' now required when using biblatex}{1}
